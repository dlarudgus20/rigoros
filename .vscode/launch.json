{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Attach to QEMU",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/kernel/bin/debug/kernel.elf",
            "cwd": "${workspaceFolder}",
            "miDebuggerServerAddress": "localhost:1234",
            "setupCommands": [
                {
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}
